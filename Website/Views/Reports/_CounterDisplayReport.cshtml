@model IList<CounterDisplay.Report>



<div class="table-responsive">
    <table id="example" class="table table-bordered text-nowrap key-buttons">
        <thead>
            <tr>
                <th>S.no.</th>
                <th>Date</th>
                <th>EMP Name</th>
                <th>EMP Code</th>
                <th>Brand</th>
                <th>Qty</th>
                <th>Image</th>
                <th>Remarks</th>
                <th>Inserted Date</th>
                <th>Inserted by</th>
                <th>Updated Date</th>
                <th>Updated by</th>
                <th>IP Address</th>
            </tr>

        </thead>
        <tbody>
            @{ int count = 0;}
            @foreach (CounterDisplay.Report item in Model)
            {
                count++;
            <tr>
                <td>@count</td>
                <td>@item.Date</td>
                <td>@item.EMPName</td>
                <td>@item.EMPCode</td>
                <td>@item.BrandName</td>
                <td>@item.Qty</td>
                <td><a href="@item.AttachmentURL" target="_blank"><img src="@item.AttachmentURL" class="img-responsive listimage" /> </a></td>

                <td>@item.Remarks</td>
                <td>@item.CreatedDate</td>
                <td>@item.CreatedBy</td>
                <td>@item.ModifiedDate</td>
                <td>@item.ModifiedBy</td>
                <td>@item.IPAddress</td>

            </tr>
            }

        </tbody>
    </table>
</div>




