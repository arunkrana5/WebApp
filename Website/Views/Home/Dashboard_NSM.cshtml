        <div class="card">
            <div class="card-body text-center">
                <div id="myfirstchartscd" class="BarChartShadow" style="height: 285px;"></div>
                <div class="row mt-5">
                    <div class="col text-center">
                        <div class="text-muted float-right"><div class="w-3 h-3 bg-primary br-3 mr-1 mt-1 float-left"></div> Target</div>
                    </div>
                    <div class="col text-center">
                        <div class="text-muted float-left"><div class="w-3 h-3 bg-secondary br-3 mr-1 mt-1 float-left"></div> Achivement</div>
                    </div>
                </div>
            </div>
        </div>
    </div><!-- col-6 -->
</div>

<div class="row" style="display:none">
    <div class="col-sm-12 col-lg-12">
        <h3 class="card-title mt-4 mb-5">Branch Wise Product Wise Sale</h3>
        <div class="card">
            <div class="card-body">
                <div class="chartjs-wrapper-demo">
                    <div id="chart3" class="h-300 mh-300"></div>
                </div>
            </div>
        </div>
    </div><!-- col-6 -->

</div>


