@model DashboardItems.ForAll


<div class="page-header">
    <div class="page-leftheader">
        <h4 class="page-title">Dashboard</h4>
    </div>

</div>
<!--End Page header-->
<!--Row-->
<div class="row">
    <div class="col-xl-4 col-md-12 col-lg-3">
        <div class="card expenses-card overflow-hidden " style="height:240px ;">
            <div class="card-body">
                <div class="feature">
                    <i class="fa fa-university feature-icon"></i>
                    <h1 class="font-weight-bold mb-0 mt-4 fs-50">@Model.TotalSales_CM</h1>
                    <p class="text-muted fs-18 mb-0">Total Sales This Month</p>
                </div>
            </div>

        </div>
    </div>
    <div class="col-xl-8 col-md-12 col-lg-9">
        <div class="row">
            <div class="col-xl-4 col-lg-4 col-md-12">
                <div class="card">
                    <div class="card-body">
                        <i class="mdi mdi-file-outline card-custom-icon icon-dropshadow-primary text-primary fs-60"></i>
                        <p class=" mb-1">Present </p>
                        <h2 class="mb-1 font-weight-bold">@Model.Present</h2>
                    </div>
                </div>
            </div>

            <div class="col-xl-4 col-lg-4 col-md-12">
                <div class="card">
                    <div class="card-body">
                        <i class="mdi mdi-clock card-custom-icon icon-dropshadow-warning text-warning fs-60"></i>
                        <p class=" mb-1">Leave </p>
                        <h2 class="mb-1 font-weight-bold">@Model.Leave</h2>
                    </div>
                </div>
            </div>
            <div class="col-xl-4 col-lg-4 col-md-12">
                <div class="card">
                    <div class="card-body">
                        <i class="mdi mdi-heart-outline card-custom-icon icon-dropshadow-success text-success fs-60"></i>
                        <p class=" mb-1">LWP</p>
                        <h2 class="mb-1 font-weight-bold">@Model.LWP</h2>

                    </div>
                </div>
            </div>
            <div class="col-xl-4 col-lg-4 col-md-12">
                <div class="card">
                    <div class="card-body">
                        <i class="mdi mdi-account-multiple-outline card-custom-icon icon-dropshadow-secondary text-secondary fs-60"></i>
                        <p class=" mb-1">Week Off</p>
                        <h2 class="mb-1 font-weight-bold">@Model.WO</h2>

                    </div>
                </div>
            </div>
            <div class="col-xl-4 col-lg-4 col-md-12">
                <div class="card">
                    <div class="card-body">
                        <i class="mdi mdi-account-multiple-outline card-custom-icon icon-dropshadow-secondary text-secondary fs-60"></i>
                        <p class=" mb-1">I am Sate</p>
                        <h2 class="mb-1 font-weight-bold">@Model.IAMSafe</h2>

                    </div>
                </div>
            </div>

            <div class="col-xl-4 col-lg-4 col-md-12">
                <div class="card">
                    <div class="card-body">
                        <i class="mdi mdi-account-multiple-outline card-custom-icon icon-dropshadow-secondary text-secondary fs-60"></i>
                        <p class=" mb-1">Absent</p>
                        <h2 class="mb-1 font-weight-bold">@Model.Absent</h2>

                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
<h3 class="card-title mt-5 mb-5">Region wise Attendance</h3>

<div class="row" style="display:none">
    <div class="col-sm-12 col-lg-3">



        <div class="card">
            <div class="card-header">
                <div class="card-title">North </div>
            </div>
            <div class="card-body">
                <div class="chartjs-wrapper-demo">
                    <div id="chart-pie" class="chartsh "></div>
                </div>
            </div>
        </div>
    </div><!-- col-6 -->

    <div class="col-sm-12 col-lg-3">
        <div class="card">
            <div class="card-header">
                <div class="card-title">East</div>
            </div>
            <div class="card-body">
                <div class="chartjs-wrapper-demo">
                    <div id="chart-pie14" class="chartsh "></div>
                </div>
            </div>
        </div>
    </div><!-- col-6 -->
    <div class="col-sm-12 col-lg-3">
        <div class="card">
            <div class="card-header">
                <div class="card-title">South</div>
            </div>
            <div class="card-body">
                <div class="chartjs-wrapper-demo">
                    <div id="chart-pie15" class="chartsh "></div>
                </div>
            </div>
        </div>
    </div><!-- col-6 -->

    <div class="col-sm-12 col-lg-3">
        <div class="card">
            <div class="card-header">
                <div class="card-title">West</div>
            </div>
            <div class="card-body">
                <div class="chartjs-wrapper-demo">
                    <div id="chart-pie16" class="chartsh "></div>
                </div>
            </div>
        </div>
    </div><!-- col-6 -->

</div>
<!--End row-->



<div class="row" style="display:none">

    <div class="col-sm-12 col-lg-6">
        <h3 class="card-title mt-4 mb-5">Region Wise Sale</h3>
        <div class="card">
            <div class="card-body">
                <div id="chart-donut" class="chartsh donutShadow"></div>
            </div>
        </div>
    </div><!-- col-6 -->


    <div class="col-sm-12 col-lg-6">
        <h3 class="card-title mt-4 mb-5">Top 5 Branch Monthly</h3>
        <div class="card">

            <div class="card-body p-5">
                <div class="d-flex align-items-end justify-content-between mg-b-5">
                    <h6 class="">Delhi</h6>
                    <h6 class="font-weight-bold mb-1">5000</h6>
                </div>
                <div class="progress progress-sm mb-5">
                    <div class="progress-bar bg-primary" style="width: 70%"></div>
                </div>
                <div class="d-flex align-items-end justify-content-between mg-b-5">
                    <h6 class="">Uttar Pardesh</h6>
                    <h6 class="font-weight-bold mb-1">3000</h6>
                </div>
                <div class="progress progress-sm mb-5">
                    <div class="progress-bar bg-secondary" style="width: 60%"></div>
                </div>
                <div class="d-flex align-items-end justify-content-between mg-b-5">
                    <h6 class="">Goa</h6>
                    <h6 class="font-weight-bold mb-1">1500</h6>
                </div>
                <div class="progress progress-sm mb-5">
                    <div class="progress-bar bg-info" style="width: 40%"></div>
                </div>
                <div class="d-flex align-items-end justify-content-between mg-b-5">
                    <h6 class="">Uttrakhand</h6>
                    <h6 class="font-weight-bold mb-1">1000</h6>
                </div>
                <div class="progress progress-sm mb-5">
                    <div class="progress-bar bg-success" style="width: 20%"></div>
                </div>
                <div class="d-flex align-items-end justify-content-between mg-b-5">
                    <h6 class="">Punjab</h6>
                    <h6 class="font-weight-bold mb-1">900</h6>
                </div>
                <div class="progress progress-sm mb-5">
                    <div class="progress-bar bg-danger" style="width: 10%"></div>
                </div>
            </div>
        </div>
    </div>


    <div class="col-sm-12">
        <h3 class="card-title mt-4 mb-5">YTD Sale</h3>
        <div class="card">
            <div class="card-body">
                <div class="chartjs-wrapper-demo">
                    <div id="chart" class="h-300 mh-300"></div>
                </div>
            </div>
        </div>
    </div><!-- col-6 -->



</div>

<div class="row" style="display:none">
    <div class="col-sm-12 col-lg-12">
        <h3 class="card-title mt-4 mb-5">Product Wise Sale</h3>
        <div class="card">
            <div class="card-body">
                <div id="chart-bar-stacked" class="chartsh h-300"></div>
            </div>
        </div>
    </div><!-- col-6 -->
</div>


<div class="row" style="display:none">
    <div class="col-sm-12 col-lg-6">
        <h3 class="card-title mt-4 mb-5">Target V/s Achivement</h3>
        <div class="card">
            <div class="card-body text-center">
                <div id="myfirstchart" class="BarChartShadow" style="height: 285px;"></div>
                <div class="row mt-5">
                    <div class="col text-center">
                        <div class="text-muted float-right"><div class="w-3 h-3 bg-primary br-3 mr-1 mt-1 float-left"></div> Target</div>
                    </div>
                    <div class="col text-center">
                        <div class="text-muted float-left"><div class="w-3 h-3 bg-secondary br-3 mr-1 mt-1 float-left"></div> Achivement</div>
                    </div>
                </div>
