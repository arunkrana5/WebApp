<div class="table-section">
    @model List<RFCEntry.List>


    <div class="table-responsive">
        <table id="example" class="table table-bordered  text-nowrap key-buttons">
            <thead>
                <tr>
                    <th>S.no.</th>
                    @if (ViewBag.Approved == 0 && ClsApplicationSetting.GetSessionValue("Delete") == "True")
                    {
                        <th>Action</th>
                    }
                    <th>Category</th>
                    <th>Doc No</th>
                    <th>Doc Date</th>
                    <th>Attendence Date</th>
                    <th>Old Attendence</th>
                    <th>New Attendence</th>
                    <th>Reason</th>
                    <th>Dealer Code</th>
                    <th>Dealer Address</th>
                    <th>Branch Code</th>
                    @if (ViewBag.Approved > 0)
                    {
                        <th>Status</th>
                        <th>Remarks</th>
                    }

                </tr>

            </thead>
            <tbody>
                @{ int count = 0;}
                @foreach (RFCEntry.List item in Model)
                {
                    count++;
                    <tr>
                        <td>@count</td>
                        @if (ViewBag.Approved == 0 && ClsApplicationSetting.GetSessionValue("Delete") == "True")
                        {
                            <td>
                                <input type="hidden" name="I" value="@item.ID" />
                                <input type="hidden" name="N" value="@item.DocNo" />
                                <a onclick="DeleteRFCEntry(this)" title="Delete Request" class="remove"><i class="fa fa-trash red-clr" aria-hidden="true"></i></a>
                            </td>
                        }
                        <td>@item.Category</td>
                        <td>@item.DocNo</td>
                        <td>@item.DocDate</td>
                        <td>@item.AttendenceDate</td>
                        <td>@item.OldStatus</td>
                        <td>@item.NewStatus</td>
                        <td>@item.Reason</td>
                        <td>@item.DealerCode</td>
                        <td>@item.DealerAddress</td>
                        <td>@item.BranchCode</td>
                        @if (ViewBag.Approved > 0)
                        {
                            <td>@item.Status</td>
                            <td>@item.ApprovedRemarks</td>

                        }
                    </tr>
                }

            </tbody>
        </table>
    </div>

</div>


