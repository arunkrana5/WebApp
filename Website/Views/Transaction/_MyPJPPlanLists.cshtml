<div class="table-section">
    @model IList<PJPPlan.List>

    <div class="table-responsive">
        <table id="example" class="table table-bordered text-nowrap key-buttons">
            <thead>
                <tr>
                    <th>S.no.</th>
                    <th>Visit Date</th>
                    <th>Status</th>
                    <th>Route Number</th>
                    <th>Visit Type</th>
                    <th>Dealer</th>
                    <th>Dealer Code</th>
                    <th>Dealer Area</th>
                    <th>Dealer Address</th>
                </tr>

            </thead>
            <tbody>
                @{ int count = 0;}
                @foreach (PJPPlan.List item in Model)
                {
                    count++;
                    <tr>
                        <td>@count</td>
                        @if (ViewBag.Approved == 1)
                        {
                            <td>
                                <a style="color:blue;" title="View" href="@Url.Action("PlanWisePJPEntries", new { src = ClsCommon.Encrypt(ViewBag.MenuID + "*/Transaction/PlanWisePJPEntries*" + item.PJPID) })" data-toggle='tooltip'>
                                    @item.VisitDate
                                </a>
                            </td>
                        }
                        else
                        {
                            <td>@item.VisitDate</td>
                        }
                        <td>@item.Status</td>
                        <td>@item.RouteNumber</td>
                        <td>@item.VisitType</td>
                        <td>@item.DealerName</td>
                        <td>@item.DealerCode</td>
                        <td>@item.DealerArea</td>
                        <td>@item.DealerAddress</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>